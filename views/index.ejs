      <!-- Tables
      ================================================== -->
      <div class="bs-docs-section">

        <div class="row">
          <div class="col-lg-12">
            <!--
            <div class="page-header">
              <h1 id="tables">Infomation</h1>
            </div>
            -->
            <div class="bs-component">

        <!-- articleArray[0]任选九场  articleArray[1]投注策略 articleArray[2]赛程安排 -->
				<% for(i in articleArray[0]) { %> 
              <table class="table table-striped table-hover ">
                <tbody>        
                  <tr>
                    <td width=5%><%=parseInt(i)+1%></td>
                    <td width=10%><%="任选九场"%></td>                    
                    <td width=15%><%=articleArray[0][i].team%></td>
                    <td width=5%><%=articleArray[0][i].select%></td>
                    <td width=50%><%=articleArray[0][i].detail%></td>
                    <td width=15%><%=articleArray[0][i].prefer%></td>                    
                  </tr>
                  <tr>
                    <td width=5%><%=parseInt(i)+1%></td>
                    <td width=10%><%="投注策略"%></td>                    
                    <td width=15%><%=articleArray[1][i].team%></td>
                    <td width=5%><%=articleArray[1][i].type%></td>
                    <td width=50%><%=articleArray[1][i].detail%></td>
                    <td width=15%><%=articleArray[1][i].prefer%></td>                    
                  </tr>
                  <tr>
                    <td width=5%><%=parseInt(i)+1%></td>
                    <td width=10%><%="赛程解读"%></td>                    
                    <td width=15%><%=articleArray[2][i].team%></td>
                    <td width=30%><%=articleArray[2][i].recent%></td>
                    <td width=30%><%=articleArray[2][i].schedule%></td>
                    <td width=10%><%=articleArray[2][i].prefer%></td>                    
                  </tr>                    
                </tbody>
              </table>
              </br>
              </br>
              </br>                                    
				<% } %>            

            </div><!-- /example -->

          </div>
        </div>
      </div>

    </div>



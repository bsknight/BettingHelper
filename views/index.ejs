      <!-- Tables
      ================================================== -->
      <div class="bs-docs-section">

        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h1 id="tables">Infomation</h1>
            </div>

            <div class="bs-component">
              <table class="table table-striped table-hover ">
                <tbody>
				<% articleArray.forEach(function (object, index) { %>
                  <tr>
                    <td width=5%><%=index%></td>
                    <td width=10%><%=object.name%></td>
                    <td width=15%><%=object.team%></td>                    
                    <% if (object.name == "赛程前瞻") { %>
                      <td width=30%><%=object.recent%></td>
                      <td width=30%><%=object.schedule%></td>
                      <td width=10%><%=object.prefer%></td>
                    <% } %>
                    <% if (object.name == "任选九场") { %>
                      <td width=5%><%=object.select%></td>
                      <td width=50%><%=object.detail%></td>
                      <td width=15%><%=object.prefer%></td>
                    <% } %>
                    <% if (object.name == "投注策略") { %>
                      <td width=5%><%=object.type%></td>
                      <td width=50%><%=object.detail%></td>
                      <td width=15%><%=object.prefer%></td>
                    <% } %>

                  </tr>
				<% }) %>  
